    <link rel="icon" href="/favicon.ico" sizes="any">
    <link rel="icon" href="/logo/logo.svg" type="image/svg+xml"/>
    {% autoescape false %}{% filter replace("\n","") | replace("    ","") | replace("<s","\n    <s") %}
